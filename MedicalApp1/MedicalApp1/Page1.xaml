<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MedicalApp1.Page1">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="name" Placeholder="Name" />
            <Entry x:Name="surname" Placeholder ="Surname" />
            <Entry x:Name="gender" Placeholder ="Gender" />
            <Entry x:Name="role" Placeholder ="Role" />
            <Entry x:Name="login" Placeholder ="Login" />
            <Button Text="Done" Clicked="OnDoneClicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>