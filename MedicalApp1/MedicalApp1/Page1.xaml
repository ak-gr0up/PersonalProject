<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MedicalApp1.Page1">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="hearthbeat" Placeholder="Hearth Beat" />
            <Entry x:Name="temperature" Placeholder ="Temperature" />
            <Entry x:Name="distal_pressure" Placeholder ="Distal Pressure" />
            <Entry x:Name="sistal_pressure" Placeholder="Sistal Pressure" />
            <Label Text="Headache" /><CheckBox x:Name="headache"/>
            <Label Text="Dizziness" /><CheckBox x:Name="Dizziness"/>
            <Label Text="Cough" /><CheckBox x:Name="cough"/>
            <Label Text="Rheum" /><CheckBox x:Name="rheum"/>
            <Label Text="Weakness" /><CheckBox x:Name="weakness"/>
            <Label Text="Nausea" /><CheckBox x:Name="nausea"/>
            <Entry x:Name="self_feeling" Placeholder ="Self Feeling" />
            <Button Text="Done" Clicked="OnDoneClicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>